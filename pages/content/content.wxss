/* pages/content/content.wxss */
.banner{
    height: 500rpx;
    position: relative;
  }
  .bannerimg{
    width: 100%;
    height: 100%;
  }
  .music1{
    width: 80rpx;
    height: 80rpx;
    position: absolute;
    left: 20rpx;
    top:20rpx;
    animation: mymusic 2s linear infinite;
  }
  .music2{
    width: 80rpx;
    height: 80rpx;
    position: absolute;
    left: 20rpx;
    top:20rpx;
  }
  @keyframes mymusic{
    0%{transform: rotate(0deg);}
    50%{transform: rotate(180deg);}
    100%{transform: rotate(360deg);}
  }
  
  .content-box{
    width: 720rpx;
    margin: 20rpx auto;
  }
  .reser{
    display: flex;
    justify-content: space-between;
    align-items: center;
    border-bottom: 1px solid #666;
    padding: 20rpx 0;
  }
  .phone{
    flex: 1;
    font-size: 18px;
  }
  .btn{
    width: 120rpx;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    border-radius: 10rpx;
    background-color: green;
    color: #fff;
    margin-left: 20rpx;
  }
  .price{
    height: 80rpx;
    line-height: 80rpx;
    padding: 20rpx 0;
    border-bottom: 1px solid #666;
    font-size: 20px;
    color: #e60303;
  }
  .small{
    height: 120rpx;
    line-height: 45rpx;
    padding: 20rpx 0;
    border-bottom: 1px solid #666; 
    text-indent: 2em;
  }
  .list .item{
    height: 80rpx;
    line-height: 80rpx;
    padding: 0 20rpx;
    border-bottom: 1px solid #666;
    display: flex;
    justify-content: space-between;
  }
  .footer{
    margin-top: 40rpx;
    text-align: center;
    color: #888;
  }
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  